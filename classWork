  function Student(name,age)
  {
      var std1={};
      std1.age=age;
      std1.name=name;
      return std1;
  }
//   s1= Student ("ali",28)
//   s2= new Student ("kamran",29)

  function constrStudent(name,age)
  {
    //  var std1={};
      this.age=age;
      this.name=name;
    //  return std1;
  }
